{
    "name": "jafrisella/wistia_api",
    "description": "Wrapper for the Wistia Data Api",
    "license": "MIT License",
    "authors": [
        {
            "name": "James Frisella",
            "email": "jafrisella@gmail.com"
        }
    ],
    "minimum-stability": "dev",
	"prefer-stable" : true,
    "require": {
		"php": ">=5.3.0",
		"ext-curl": "*"
	},
	"require-dev": {
        "phpunit/phpunit": "5.0.*"
    },
	"autoload":{
		"classmap":[
			"src/Wistia",
			"src/Interfaces",
			"src/Resources",
			"src/Resources/Account",
			"src/Resources/Captions",
			"src/Resources/Customizations",
			"src/Resources/Medias",
			"src/Resources/Projects",
			"src/Resources/Sharings",
			"src/Utility/Curl",
			"src/Utility/Output",
			"src/Utility/Uri"
		]
	}
}
